<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  
	<servlet>
		<servlet-name>fileupServlet</servlet-name>
		<servlet-class>main.FileUpServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>fileDownServlet</servlet-name>
		<servlet-class>main.FileDownServlet</servlet-class>
	</servlet>
	
	
	<servlet-mapping>
		<servlet-name>fileupServlet</servlet-name>
		<url-pattern>/fileup</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>fileDownServlet</servlet-name>
		<url-pattern>/fileDown</url-pattern>
	</servlet-mapping>
</web-app>